{
  "name": "2associes-meteor-boilerplate",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "meteor run --settings settings-development.json",
    "dev:test": "meteor run --settings settings-test.json",
    "dev:prod": "meteor run --production --settings settings.json",
    "test": "meteor test --full-app --driver-package tmeasday:acceptance-test-driver --settings settings-development.json",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "pretest": "npm run lint --silent"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.js",
      "server": "server/main.js"
    }
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "aws-sdk": "^2.648.0",
    "bcrypt": "^4.0.1",
    "bootstrap": ">=4.3.1",
    "circle-github-bot": "^2.0.1",
    "fibers": "^4.0.2",
    "font-awesome": "^4.7.0",
    "handlebars": "^4.7.3",
    "jquery": "^3.4.1",
    "meteor-node-stubs": "^1.0.0",
    "mjml": "^4.5.1",
    "moment": "^2.22.2",
    "popper.js": "^1.14.5",
    "simpl-schema": "^1.5.3",
    "wait-on": "^4.0.1"
  },
  "devDependencies": {
    "@wdio/cli": "^5.22.3",
    "@wdio/junit-reporter": "^5.22.4",
    "@wdio/local-runner": "^5.22.3",
    "@wdio/mocha-framework": "^5.18.7",
    "@wdio/spec-reporter": "^5.22.4",
    "@wdio/sync": "^5.20.1",
    "autoprefixer": "^9.3.1",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "chromedriver": "^80.0.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-meteor": "^6.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "isomorphic-ws": "^4.0.1",
    "simpleddp": "^2.2.4",
    "wdio-chromedriver-service": "^5.0.2",
    "ws": "^7.2.3"
  },
  "lighthouse": {
    "requiredScores": {
      "performance": 50,
      "accessibility": 50,
      "best-practices": 50,
      "seo": 50
    }
  }
}
