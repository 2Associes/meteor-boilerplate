{
  "name": "2associes-meteor-boilerplate",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "meteor run --settings settings-development.json",
    "simulate:prod": "meteor run --production --settings settings.json",
    "test": "chimp --ddp=http://localhost:3000 --browser=chrome --path=tests",
    "test-watch": "chimp --watch --ddp=http://localhost:3000 --browser=chrome --path=tests",
    "dev:test": "meteor run --settings settings-test.json",
    "dev:prod": "meteor run --production --settings settings.json",
    "test": "meteor test --full-app --driver-package tmeasday:acceptance-test-driver --settings settings-development.json",
    "chimp-watch": "node_modules/.bin/chimpy --ddp=http://localhost:3000 --watch --mocha --path=tests",
    "chimp-test": "node_modules/.bin/chimpy ./tests/_chimp_config.js --ddp=http://localhost:3000 --mocha --path=tests",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "pretest": "npm run lint --silent"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions"
        ]
      }
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "babel-runtime": "^6.23.0",
    "bcrypt": "^3.0.2",
    "bootstrap": ">=4.3.1",
    "chimp": "^0.51.1",
    "core-js": "^2.5.7",
    "font-awesome": "^4.7.0",
    "handlebars": "^4.7.3",
    "jquery": "^3.3.1",
    "meteor-node-stubs": "^0.4.1",
    "mjml": "^4.5.1",
    "moment": "^2.22.2",
    "popper.js": "^1.14.5",
    "simpl-schema": "^1.5.3",
    "wait-on": "^4.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "babel-eslint": "^10.0.3",
    "chimpy": "^3.0.1",
    "chromedriver": "^80.0.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-meteor": "^6.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1"
  }
}
